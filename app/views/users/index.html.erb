<h1 align="center">Users of Pelco System</h1>

<div align="center">
    <%#= will_paginate @users %>
	<% @users.each do |user| %>
	  <ul class="listing">
	  	<div class="row">
	  		<div class="well col-md-4 col-md-offset-4">
	  			<li class="article-title">
	  				<%= link_to user.email, user_path(user) %>
	  			</li>
	  			
	  		</div>
	  	</div>
	  </ul>
	<% end %>
    <%#= will_paginate @users %>
</div>