<%= form_for @nvr do |f| %>

  <%= f.label "名称" %>
  <%= f.text_field :name %>

  <%= f.label "IP" %>
  <%= f.text_field :nvr_ip %>


  <%= f.label "端口" %>
  <%= f.text_field :nvr_port %>

 
  <%= f.label "用户名" %>
  <%= f.text_field :nvr_user %>


  <%= f.label "密码" %>
  <%= f.password_field :password %>

  <%= f.submit "Save Changes" %>

<% end %>